[global_config]
  title_transmit_fg_color = "#dedede"
  title_transmit_bg_color = "#2e3436"
  title_inactive_fg_color = "#dedede"
  title_inactive_bg_color = "#555753"

[profiles]
  [[default]]
    palette = "#2e3436:#cc0000:#4e9a06:#c4a000:#3465a4:#75507b:#06989a:#d3d7cf:#555753:#ef2929:#8ae234:#fce94f:#729fcf:#ad7fa8:#34e2e2:#eeeeec"
    foreground_color = "#dedede"
    background_color = "#2b2b2b"
    cursor_color = "#dedede"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = default
    [[[window0]]]
      type = Window
      parent = ""
