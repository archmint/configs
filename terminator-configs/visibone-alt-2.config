[global_config]
  title_transmit_fg_color = "#cccccc"
  title_transmit_bg_color = "#666666"
  title_inactive_fg_color = "#cccccc"
  title_inactive_bg_color = "#999999"

[profiles]
  [[default]]
    palette = "#666666:#cc6699:#99cc66:#cc9966:#6699cc:#9966cc:#66cc99:#cccccc:#999999:#ff99cc:#ccff99:#ffcc99:#99ccff:#cc99ff:#99ffcc:#ffffff"
    foreground_color = "#cccccc"
    background_color = "#333333"
    cursor_color = "#cccccc"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = default
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
