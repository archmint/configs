[global_config]
  title_transmit_fg_color = "#ffffff"
  title_transmit_bg_color = "#393939"
  title_inactive_fg_color = "#ffffff"
  title_inactive_bg_color = "#474747"

[profiles]
  [[default]]
    palette = "#393939:#da4939:#519f50:#cc7833:#6d9cbe:#9f5079:#435d75:#dddddd:#474747:#ff6c5c:#a5c261:#bc9458:#d0d0ff:#a761c2:#6e98a4:#dddddd"
    foreground_color = "#ffffff"
    background_color = "#000000"
