[global_config]
  title_transmit_fg_color = "#ffffff"
  title_transmit_bg_color = "#202020"
  title_inactive_fg_color = "#ffffff"
  title_inactive_bg_color = "#505050"

[profiles]
  [[default]]
    palette = "#202020:#a07070:#70a070:#a0a070:#7070a0:#a070a0:#70a0a0:#a0a0a0:#505050:#d07070:#70d070:#d0d070:#7070d0:#d070d0:#70d0d0:#f0f0f0"
    foreground_color = "#ffffff"
    background_color = "#000000"
    cursor_color = "#ffffff"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = default
    [[[window0]]]
      type = Window
      parent = ""
