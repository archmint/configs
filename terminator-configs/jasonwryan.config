[global_config]
  title_transmit_fg_color = "#ffffff"
  title_transmit_bg_color = "#222222"
  title_inactive_fg_color = "#ffffff"
  title_inactive_bg_color = "#454545"

[profiles]
  [[default]]
    palette = "#222222:#9e5641:#6c7e55:#caaf2b:#4c8ea1:#956d9d:#7c9aa6:#909090:#454545:#cc896d:#7db37d:#bfb556:#6bc1d0:#c18fcb:#8fadbf:#ffffff"
    foreground_color = "#ffffff"
    background_color = "#000000"
    cursor_color = "#ffffff"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = default
    [[[window0]]]
      type = Window
      parent = ""
